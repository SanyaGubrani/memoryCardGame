{
  "compilerOptions": {
      "target": "ES2022",
      "lib": ["ES2023"],
      "module": "ESNext",
      "skipLibCheck": true,
      "composite": true,
      "moduleResolution": "bundler",
      "allowImportingTsExtensions": false,
      "isolatedModules": true,
      "moduleDetection": "force",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,
      "outDir": "dist",
      "noEmit": false
  },
  "include": ["vite.config.ts"]
}